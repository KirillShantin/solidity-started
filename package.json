{
  "author": "Andrey Gulitsky <andrey@gulisky.dev> (https://gulitsky.dev)",
  "bugs": "https://github.com/gulitsky/solidity-starter/issues",
  "dependencies": {
    "@ethersproject/abi": "^5.5.0",
    "@ethersproject/bytes": "^5.5.0",
    "@ethersproject/providers": "^5.5.0",
    "ethers": "^5.5.1"
  },
  "description": "",
  "devDependencies": {
    "@codechecks/client": "^0.1.12",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.11",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@swc/helpers": "^0.2.13",
    "@typechain/ethers-v5": "^8.0.1",
    "@typechain/hardhat": "^3.0.0",
    "@types/chai": "^4.2.22",
    "@types/fs-extra": "^9.0.13",
    "@types/is-ci": "^3.0.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.6",
    "chai": "^4.3.4",
    "chalk": "^4.1.2",
    "copyfiles": "^2.4.1",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.1",
    "fs-extra": "^10.0.0",
    "hardhat": "^2.6.7",
    "hardhat-deploy": "^0.9.4",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-packager": "^1.2.1",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "lint-staged": "^11.2.6",
    "live-server": "^1.2.1",
    "mocha": "^9.1.3",
    "mocha-junit-reporter": "^2.0.2",
    "pinst": "^2.1.6",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-beta.18",
    "shx": "^0.3.3",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.17",
    "ts-node": "gulitsky/sw-node",
    "tslib": "^2.3.1",
    "typechain": "^6.0.2",
    "typescript": "~4.4.4"
  },
  "engines": {
    "node": ">=15.0.0",
    "npm": "please-use-yarn",
    "yarn": ">=1.22.0"
  },
  "exports": {
    "./types/": "./dist/types/"
  },
  "files": [
    "./artifacts/",
    "./contracts/",
    "./dist/**/*.d.ts",
    "./dist/**/*.d.ts.map",
    "./dist/**/*.js",
    "./dist/**/*.js.map"
  ],
  "homepage": "https://github.com/gulitsky/solidity-starter#readme",
  "keywords": [
    "blockchain",
    "ethereum",
    "hardhat",
    "smart-contracts",
    "solidity"
  ],
  "license": "ISC",
  "main": "./dist/index.js",
  "name": "@gulitsky/solidity-starter",
  "packageManager": "yarn@3.1.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": "github:gulitsky/solidity-starter",
  "scripts": {
    "build": "hardhat compile",
    "clean": "shx rm -rf ./artifacts/ ./cache/ ./coverage/ ./dist/ ./coverage.json ./gasReporterOutput.json ./gas-profile-report.txt ./test-results.xml ./tsconfig.prod.tsbuildinfo",
    "coverage": "hardhat coverage --solcoverjs ./.solcover.js --temp ./artifacts/ --testfiles \"./test/**/*.ts\"",
    "coverage:watch": "live-server -q ./coverage/lcov-report/",
    "deploy": "hardhat deploy",
    "lint:sol": "solhint --config ./.solhint.json --max-warnings 0 \"./contracts/**/*.sol\"",
    "lint": "yarn run lint:sol && yarn prettier",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "build:types": "hardhat prepare-package && tsc --project ./tsconfig.build.json && copyfiles --up 1 \"./src/**/*.d.ts\" ./dist/",
    "prettier": "prettier --check --config ./.prettierrc  \"./**/*.{js,json,md,sol,ts,yml}\"",
    "prettier:fix": "prettier --config ./.prettierrc --write \"./**/*.{js,json,md,sol,ts,yml}\"",
    "profile": "REPORT_GAS=true CI=1 yarn run test",
    "rebuild": "yarn run build --force",
    "test": "hardhat test",
    "typechain": "hardhat typechain",
    "verify": "hardhat etherscan-verify --solc-input"
  },
  "types": "./dist/index.d.ts",
  "version": "0.0.0"
}
